    "eslint": "^8.57.0",
    "mime": "^4.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.8"
  },
  "dependencies": {
    "@viz-js/viz": "^3.4.0"
  }
